{
  "name": "home-web-apps",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "docker:publish": "pnpm run docker:build && pnpm run docker:push",
    "docker:build": "turbo run docker:build",
    "docker:push": "turbo run docker:push",
    "clean": "turbo run clean",
    "dev": "turbo run dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "storybook": "turbo run storybook",
    "graph": "turbo graph"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.11",
    "eslint": "^9.16.0",
    "prettier": "^3.2.5",
    "turbo": "^2.0.9"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  },
  "packageManager": "pnpm@9.15.4"
}
